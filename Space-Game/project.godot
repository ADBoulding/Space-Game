; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Character",
"language": "GDScript",
"path": "res://scripts/Player/character.gd"
}, {
"base": "Reference",
"class": "Ship",
"language": "GDScript",
"path": "res://scripts/core/ship.gd"
} ]
_global_script_class_icons={
"Character": "",
"Ship": ""
}

[application]

config/name="Space-Game"
run/main_scene="res://Scenes/title_screen.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://scripts/core/Global.gd"
NameGenerator="*res://scripts/core/randomNameGenerator.gd"
Galaxy="*res://scripts/galaxy/Galaxy.gd"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input]

ui_end={
"deadzone": 0.5,
"events": [  ]
}

[rendering]

quality/2d/use_pixel_snap=true
environment/default_clear_color=Color( 0.14902, 0.192157, 0.219608, 1 )
environment/default_environment="res://default_env.tres"
