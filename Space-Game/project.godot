config_version=4

_global_script_classes=[ {
"base": "VisualShaderNodeCustom",
"class": "VisualShaderNodePerlinNoise3D",
"language": "GDScript",
"path": "res://Scenes/Planet_Gen/noise_script.gd"
} ]
_global_script_class_icons={
"VisualShaderNodePerlinNoise3D": ""
}

[application]

config/name="Space-Game"
run/main_scene="res://Scenes/title_screen.tscn"
